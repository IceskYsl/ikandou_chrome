{
  "name": "爱看豆找书",
  "version": "0.2",
  "manifest_version": 2,
  "description": "用爱看豆搜索电子书资源",
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "content_scripts": [
    {
        "matches": ["http://book.douban.com/subject/*/"],
        "js": ["jquery-1.8.3.min.js", "search.js"],
        "css": ["ikandou.css"],
        "run_at": "document_end"
    },
    {
        "matches": ["http://book.douban.com/people/*/wish*",
                    "http://book.douban.com/people/*/do*",
                    "http://book.douban.com/people/*/collect*"
                   ],
        "js": ["jquery-1.8.3.min.js", "doulist.js"],
        "css": ["ikandou.css"],
        "run_at": "document_end"
    }
  ],
  "permissions": ["http://ikandou.com/api/*"],
  "web_accessible_resources": [
    "templates/book_page_widget.html"
  ]
}
