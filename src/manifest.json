{
  "name": "爱看豆找书",
  "version": "0.1",
  "manifest_version": 2,
  "description": "用爱看豆搜索电子书资源",
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "content_scripts": [
    {
        "matches": ["http://book.douban.com/subject/*/"],
        "js": ["jquery-1.8.3.min.js", "search.js"],
        "css": ["ikandou.css"],
        "run_at": "document_end"
    }
  ],
  "permissions": ["http://ikandou.com/api/*"]
}
